#include <stdio.h>

int main(void)
{
    int nums [10];
    printf("Enter the numbers\n");
    for(int i=0;i<10;i++){
            scanf("%d",&nums[i]); }
    int max=nums [0];
    for(int i=0;i<10; i++)
        { if(nums[i]>max) {
            max=nums[i];
}
else{
        continue; }

}
printf("Largest Value: %d\n",max);
return 0;
}
